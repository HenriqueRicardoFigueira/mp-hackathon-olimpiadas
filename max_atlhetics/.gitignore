# Ignore bundler config.
/.bundle
vendor/

lib/dbt_core/venv

# Ignore all logfiles and tempfiles.
/log/*
/logs/*
/tmp/*
!/log/.keep
!/tmp/.keep


# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials/staging.key

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
coverage
rspec.xml
.env
.env-dbt
.env-pgsync
.env.test
.env.development
.idea
.irb_history

#Ignore database.yml
config/database.yml

# Elastic Beanstalk Files
.elasticbeanstalk/*
!.elasticbeanstalk/*.cfg.yml
!.elasticbeanstalk/*.global.yml
.DS_Store
.github/.DS_Store

